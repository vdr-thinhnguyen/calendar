{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "baseUrl": ".",
        "paths": {
            "@components/*": ["components/*"],
            "@pages/*": ["pages/*"],
            "@widgets/*": ["widgets/*"],
            "@configs/*": ["configs/*"],
            "@layouts/*": ["layouts/*"],
            "@hooks/*": ["hooks/*"],
        }
    },
    "exclude": ["node_modules"]
}